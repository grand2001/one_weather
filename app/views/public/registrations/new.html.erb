<h2>新規登録</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "public/shared/error_messages", resource: resource %>

  <div class="field">
   <div><%= f.label :email, "メールアドレス", class: "" %></div>
   <div><%= f.email_field :email, autofocus: true, autocomplete: "email" %></div>
  </div>

  <div class="field">
   <div><%= f.label :password, "パスワード", class: "" %></div>
   <div><%= f.password_field :password, autocomplete: "new-password" %></div>
  </div>

  <div class="field">
   <div><%= f.label :password_confirmation, "パスワード(確認用)", class: "" %></div>
   <div><%= f.password_field :password_confirmation, autocomplete: "new-password" %></div>
  </div>

  <div class="actions">
   <%= f.submit "新規登録" %>
  </div>
<% end %>

<%= render "public/shared/links" %>
